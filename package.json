{
  "name": "jwks-rsa",
  "version": "1.3.0",
  "description": "Library to retrieve RSA public keys from a JWKS endpoint",
  "main": "lib/index.js",
  "types": "index.d.ts",
  "dependencies": {
    "debug": "^2.2.0",
    "limiter": "^1.1.0",
    "lru-memoizer": "^1.6.0",
    "ms": "^2.0.0",
    "request": "^2.73.0"
  },
  "devDependencies": {
    "@types/express-jwt": "0.0.34",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.10.2",
    "eslint-plugin-babel": "^3.2.0",
    "express-jwt": "^3.4.0",
    "jsonwebtoken": "^7.1.7",
    "koa": "^2.2.0",
