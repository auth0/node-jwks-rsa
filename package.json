{
  "name": "jwks-rsa",
  "version": "3.1.0",
  "description": "Library to retrieve RSA public keys from a JWKS endpoint",
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/esm/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "jose": "^4.14.6",
    "limiter": "^1.1.5",
    "lru-memoizer": "^2.2.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^14.1.0",
    "@types/chai-as-promised": "^7.1.6",
    "@types/chai": "^4.3.6",
    "@types/debug": "^4.1.9",
    "@types/express-jwt-v6": "npm:@types/express-jwt@^6.0.4",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.9",
    "@types/mocha": "^10.0.1",
    "@types/nock": "^11.0.0",
    "@types/node": "^20.6.0",
    "@types/passport-jwt": "^3.0.10",
    "@types/passport": "^1.0.13",
    "@types/supertest": "^2.0.14",
    "chai-as-promised": "^7.1.1",
    "chai": "^4.3.8",
    "eslint": "^8.49.0",
    "express-jwt-v6": "npm:express-jwt@^6.1.2",
    "express-jwt-v7": "npm:express-jwt@^7.5.0",
    "express-jwt": "^8.4.1",
    "express": "^4.18.2",
    "jose2": "npm:jose@^2.0.6",
    "jsonwebtoken": "^9.0.2",
    "koa-jwt": "^4.0.4",
    "koa": "^2.14.2",
    "mocha": "^10.2.0",
    "nock": "^13.3.3",
    "nyc": "^15.1.0",
    "passport-jwt": "^4.0.1",
    "passport": "^0.6.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "rimraf dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.build-cjs.json && cp src/types.d.ts dist/cjs/types.d.ts && echo '{ \"type\": \"commonjs\" }'> dist/cjs/package.json",
    "build:esm": "tsc --project tsconfig.build-esm.json && cp src/types.d.ts dist/esm/types.d.ts",
    "clean:ts": "rimraf ts-output/",
    "lint": "eslint ./src ./tests",
    "test:ts": "npm run clean:ts && tsc --project tsconfig.tests.json && NODE_ENV=test mocha --exit --timeout 5000 $(find ./ts-output -name *.tests.js)",
    "test:js": "NODE_ENV=test mocha --exit --timeout 5000 $(find ./tests -name *.tests.js)",
    "test": "npm run test:js && npm run test:ts",
    "test:ci": "nyc --reporter=lcov npm test",
    "test-watch": "NODE_ENV=test mocha --exit --timeout 5000 $(find ./tests -name *.tests.js) --watch",
    "types:check": "tsc",
    "release": "git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auth0/node-jwks-rsa.git"
  },
  "keywords": [
    "jwks",
    "rsa",
    "jwt"
  ],
  "author": "Auth0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/auth0/node-jwks-rsa/issues"
  },
  "homepage": "https://github.com/auth0/node-jwks-rsa#readme"
}
